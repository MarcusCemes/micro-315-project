{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Compile",
      "type": "shell",
      "command": "make",
      "options": {
        "cwd": "${workspaceRoot}/micro-315-project"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$gcc"
    },
    {
      "label": "Clean",
      "type": "shell",
      "command": "make clean",
      "options": {
        "cwd": "${workspaceRoot}/micro-315-project"
      },
      "problemMatcher": []
    }
  ]
}
